{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "maxLength": 15,
      "minLength": 2
    },
    "lastName": {
      "type": "string",
      "required": true,
      "maxLength": 20,
      "minLength": 2
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "joined": {
      "type": "date",
      "default": "2022-05-01"
    },
    "additionalMember": {
      "type": "component",
      "repeatable": false,
      "component": "member-info.additional-member"
    }
  }
}
